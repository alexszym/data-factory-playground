{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-data-factory-123"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azure-data-factory-kvt.vault.azure.net/"
		},
		"blob-storage_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/7b37a8d6-5e6d-4466-a3d5-4beb1190f7a4/resourceGroups/Dev/providers/Microsoft.Storage/storageAccounts/mydevstorage123"
		},
		"blob-storage_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"sql-test-db_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/7b37a8d6-5e6d-4466-a3d5-4beb1190f7a4/resourceGroups/Dev/providers/Microsoft.Sql/servers/dev-sql-server-123"
		},
		"sql-test-db_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"sql-test-data_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/7b37a8d6-5e6d-4466-a3d5-4beb1190f7a4/resourceGroups/Dev/providers/Microsoft.Sql/servers/dev-sql-server-124"
		},
		"sql-test-data_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dev-data-factory-123_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"blob-storage_properties_privateLinkResourceId": {
						"value": "[parameters('blob-storage_properties_privateLinkResourceId')]"
					},
					"blob-storage_properties_groupId": {
						"value": "[parameters('blob-storage_properties_groupId')]"
					},
					"sql-test-db_properties_privateLinkResourceId": {
						"value": "[parameters('sql-test-db_properties_privateLinkResourceId')]"
					},
					"sql-test-db_properties_groupId": {
						"value": "[parameters('sql-test-db_properties_groupId')]"
					},
					"sql-test-data_properties_privateLinkResourceId": {
						"value": "[parameters('sql-test-data_properties_privateLinkResourceId')]"
					},
					"sql-test-data_properties_groupId": {
						"value": "[parameters('sql-test-data_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}